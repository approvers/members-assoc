{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "strict": true,
        "lib": ["esnext"],
        "types": ["@cloudflare/workers-types"],
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "verbatimModuleSyntax": true,
        "noEmit": true,
        "skipLibCheck": true
    }
}
